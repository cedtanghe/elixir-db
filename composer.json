{
    "name": "elixir/elixir-db",
    "description": "Component database and ORM",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "database",
        "ORM"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-db",
    "require": {
        "php": ">=5.4",
        "elixir/elixir-stdlib": "~1.0",
        "elixir/elixir-dispatcher": "~1.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\DB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\DB\\": "tests/"
        }
    },
    "suggest": {
        "elixir/elixir-di": "~1.0"
    }
}